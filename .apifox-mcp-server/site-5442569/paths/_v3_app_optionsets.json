{
  "get": {
    "summary": "获取选项集列表",
    "deprecated": false,
    "description": "获取应用下选项集列表",
    "tags": [
      "选项集"
    ],
    "parameters": [
      {
        "name": "HAP-Appkey",
        "in": "header",
        "description": "appkey",
        "required": false,
        "example": "{{appkey}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Sign",
        "in": "header",
        "description": "签名",
        "required": false,
        "example": "{{sign}}",
        "schema": {
          "type": "string"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "optionsets": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/obj_optionset"
                      }
                    }
                  },
                  "required": [
                    "optionsets"
                  ]
                },
                "success": {
                  "type": "boolean",
                  "description": "是否调用成功"
                },
                "error_code": {
                  "type": "integer",
                  "description": "错误代码"
                },
                "error_msg": {
                  "type": "string",
                  "description": "错误消息"
                }
              },
              "required": [
                "data",
                "success"
              ]
            },
            "example": {
              "data": {
                "optionsets": [
                  {
                    "id": "",
                    "name": "",
                    "accountId": "",
                    "worksheetIds": [],
                    "options": [
                      {
                        "key": "",
                        "value": "",
                        "index": 23,
                        "isDeleted": false,
                        "color": "",
                        "score": 64
                      }
                    ],
                    "enableColor": true,
                    "enableScore": false
                  }
                ]
              },
              "success": true
            }
          }
        },
        "headers": {}
      }
    },
    "security": []
  },
  "post": {
    "summary": "创建选项集",
    "deprecated": false,
    "description": "创建选项集信息\n数据包括：选项集名称、选项集信息（选项值、排序、颜色、分值）",
    "tags": [
      "选项集"
    ],
    "parameters": [
      {
        "name": "HAP-Appkey",
        "in": "header",
        "description": "appkey",
        "required": false,
        "example": "{{appkey}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Sign",
        "in": "header",
        "description": "签名",
        "required": false,
        "example": "{{sign}}",
        "schema": {
          "type": "string"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "选项集名称"
              },
              "options": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "value": {
                      "type": "string",
                      "description": "选项值，不允许重复"
                    },
                    "index": {
                      "type": "integer",
                      "description": "选项排序值: 必须为整数，越小越靠前"
                    },
                    "color": {
                      "type": "string",
                      "description": "颜色值: colorful为true时生效，参考值 #C0E6FC , #C3F2F2 , #00C345 , #FAD714 , #FF9300 , #F52222 , #EB2F96 , #7500EA , #2D46C4 , #484848 , #C9E6FC , #C3F2F2"
                    },
                    "score": {
                      "type": "number",
                      "description": "分值，enableScore为true时生效，允许小数和正负值"
                    }
                  },
                  "required": [
                    "value",
                    "index",
                    "color",
                    "score"
                  ]
                },
                "description": "选项集"
              },
              "enableColor": {
                "type": "boolean",
                "description": "启用彩色"
              },
              "enableScore": {
                "type": "boolean",
                "description": "启用分值"
              }
            },
            "required": [
              "name",
              "options",
              "enableColor",
              "enableScore"
            ]
          },
          "example": {
            "name": "测试选项集",
            "options": [
              {
                "value": "已通过",
                "index": "1",
                "color": "#C3F2F2",
                "score": "1"
              },
              {
                "value": "不通过",
                "index": 1,
                "color": "red",
                "score": 1.5
              }
            ],
            "colorful": false,
            "enableScore": false
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "optionsetId": {
                      "type": "string",
                      "description": "选项集 ID"
                    }
                  },
                  "required": [
                    "optionsetId"
                  ]
                },
                "success": {
                  "type": "boolean",
                  "description": "是否调用成功"
                },
                "error_code": {
                  "type": "integer",
                  "description": "错误代码"
                },
                "error_msg": {
                  "type": "string",
                  "description": "错误消息"
                }
              },
              "required": [
                "data",
                "success"
              ]
            },
            "example": {
              "data": {
                "optionsetId": ""
              },
              "success": true
            }
          }
        },
        "headers": {}
      }
    },
    "security": []
  }
}