{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "title": "",
      "description": "字段名称"
    },
    "alias": {
      "type": "string",
      "title": "",
      "description": "字段别名"
    },
    "type": {
      "type": "string",
      "description": "字段类型"
    },
    "isTitle": {
      "type": "boolean",
      "description": "是否是标题字段"
    },
    "required": {
      "type": "boolean",
      "description": "是否必填"
    },
    "isHidden": {
      "type": "boolean",
      "description": "是否隐藏"
    },
    "isReadOnly": {
      "type": "boolean",
      "description": "是否只读"
    },
    "isHiddenOnCreate": {
      "type": "boolean",
      "description": "是否新增记录时隐藏"
    },
    "isUnique": {
      "type": "boolean",
      "description": "是否唯一"
    },
    "precision": {
      "type": "integer",
      "description": "type=Number时，表示保留小数位（0-14）\n"
    },
    "subType": {
      "type": "string",
      "description": "type=Collaborator时，表示成员数量，填入规则 0：单选,1：多选\ntype=Relation时，表示关联记录数量，填入规则 1：单条 2：多条\ntype=Time时，表示时间1：时.分 ，6：时.分.秒\ntype=Date|DateTime时，表示日期：5：年 4：年月 3：年月日 2：年月日时 1：年月日时分 6：年月日时分秒"
    },
    "options": {
      "type": "object",
      "properties": {
        "value": {
          "type": "string",
          "description": "选项名称"
        },
        "index": {
          "description": "选项排序",
          "type": "integer"
        }
      },
      "description": "type=SingleSelect|MultipleSelect时传入， 11:单选,10:多选时填入"
    },
    "showFields": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "description": "type=Relation时传入关联表 id，表示展示字段\n"
    },
    "max": {
      "type": "integer",
      "description": "type=Rating时传入，表示等级最大值，填入规则（0-10）"
    },
    "dataSource": {
      "type": "string",
      "description": "type=Relation时传入关联表 id，表示关联表 id"
    },
    "sourceControlId": {
      "type": "string",
      "description": "type=Relation时传入，关联字段字段 id，用于创建双向关联\n\n"
    }
  }
}