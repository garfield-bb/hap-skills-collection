        "description": "",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Appkey",
        "in": "header",
        "description": "appkey",
        "required": false,
        "example": "{{appkey}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Sign",
        "in": "header",
        "description": "签名",
        "required": false,
        "example": "{{sign}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Appid",
        "in": "header",
        "description": "当使用Authorization鉴权时，必传",
