        "example": "{{appId}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "Authorization",
        "in": "header",
        "description": "个人授权 id",
        "required": false,
        "example": "{{Authorization}}",
        "schema": {
          "type": "string"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "optionsets": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/options"
                      }
                    }
                  },
                  "required": [
                    "optionsets"
                  ]
                },
                "success": {
                  "type": "boolean"
                },
                "errorCode": {
                  "type": "string"
                },
                "errorMsg": {
                  "type": "string"
                }
              },
              "required": [
                "data",
                "success"
              ]
            },
            "example": "{\n    \"data\": {\n        \"optionsets\": [\n            {\n                \"appId\": \"84ab0007-53be-42d0-abb1-ca48d5914823\",\n                \"organizationId\": \"28447959-cf7f-4ae9-9074-8977ce48e2a0\",\n                \"collectionId\": \"052ca8d0-9a3e-43e5-ac6f-9158256e3345\",\n                \"name\": \"订单状态\",\n                \"accountId\": \"e2453de8-1a6a-4d4f-ba36-87fdff880a20\",\n                \"worksheetIds\": [],\n                \"options\": [\n                    {\n                        \"key\": \"54cb74a9-022c-484a-a7c2-68a67ec6ed92\",\n                        \"value\": \"待支付\",\n                        \"index\": 1,\n                        \"isDeleted\": false,\n                        \"color\": \"#C9E6FC\",\n                        \"score\": 0.0,\n                        \"hide\": false\n                    },\n                    {\n                        \"key\": \"8798bc55-51e7-48c8-8869-e53496263ebd\",\n                        \"value\": \"已取消\",\n                        \"index\": 2,\n                        \"isDeleted\": false,\n                        \"color\": \"#C3F2F2\",\n                        \"score\": 0.0,\n                        \"hide\": false\n                    },\n                    {\n                        \"key\": \"14b812fa-4701-455f-8589-9f96e90f1e94\",\n                        \"value\": \"已完成\",\n                        \"index\": 3,\n                        \"isDeleted\": false,\n                        \"color\": \"#00C345\",\n                        \"score\": 0.0,\n                        \"hide\": false\n                    }\n                ]\n            }\n        ],\n        \"enableColor\": false,\n        \"enableScore\": false\n    }\n}\n  \"success\": true\n}"
          }
        },
        "headers": {}
      }
    },
    "security": []
  },
  "post": {
    "summary": "创建选项集",
    "deprecated": false,
    "description": "创建选项集信息\n数据包括：选项集名称、选项集信息（选项值、排序、颜色、分值）",
    "tags": [],
    "parameters": [
      {
        "name": "url",
        "in": "query",
        "description": "",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Appkey",
        "in": "header",
        "description": "appkey",
        "required": false,
        "example": "{{appkey}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Sign",
        "in": "header",
        "description": "签名",
        "required": false,
        "example": "{{sign}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "HAP-Appid",
        "in": "header",
        "description": "当使用Authorization鉴权时，必传",
        "required": false,
        "example": "{{appId}}",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "Authorization",
        "in": "header",
        "description": "个人授权 id",
        "required": false,
        "example": "{{Authorization}}",
        "schema": {
          "type": "string"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string",
                "description": "选项集名称"
              },
              "options": {
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "value": {
                      "type": "string",
                      "description": "选项值，不允许重复"
                    },
                    "index": {
                      "type": "string",
                      "description": "选项排序值: 必须为整数，越小越靠前"
                    },
                    "color": {
                      "type": "string",
                      "description": "颜色值: colorful为true时生效，参考值 #C0E6FC , #C3F2F2 , #00C345 , #FAD714 , #FF9300 , #F52222 , #EB2F96 , #7500EA , #2D46C4 , #484848 , #C9E6FC , #C3F2F2"
                    },
                    "score": {
                      "type": "string",
                      "description": "分值，enableScore为true时生效，允许小数和正负值"
                    }
                  },
                  "required": [
                    "value",
                    "index",
                    "color",
                    "score"
                  ]
                },
                "description": "选项集"
              },
              "enableColor": {
                "type": "boolean",
                "description": "启用彩色"
              },
              "enableScore": {
                "type": "boolean",
                "description": "启用分值"
              }
            },
            "required": [
              "name",
              "options",
              "enableColor",
              "enableScore"
            ]
          },
          "example": {
            "name": "测试选项集",
            "options": [
              {
                "value": "已通过",
                "index": "1",
                "color": "#C3F2F2",
                "score": "1"
              },
              {
                "value": "不通过",
                "index": 1,
                "color": "red",
                "score": 1.5
              }
            ],
            "colorful": false,
            "enableScore": false
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "object",
                  "properties": {
                    "optionsetId": {
                      "type": "string",
                      "description": "选项集 ID"
                    }
                  },
                  "required": [
                    "optionsetId"
                  ]
                },
                "success": {
                  "type": "boolean"
                },
                "errorCode": {
                  "type": "string"
                },
                "errorMsg": {
                  "type": "string"
                }
              },
              "required": [
                "success",
                "data"
              ]
            },
            "example": {
              "data": {
                "optionsetId": ""
              },
              "success": true
            }
          }
        },
        "headers": {}
      }
    },
    "security": []
  }
}